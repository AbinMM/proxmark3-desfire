{
	.start : { obj/start.o(.text) } >osimage
	.text : { 
		*(.text)
		*(.text.*)
		*(.glue_7)
		*(.glue_7t)
	} >osimage
	.rodata : { 
		*(.rodata) 
		*(.rodata*) 
	} >osimage
	.data : { *(.data) } >ram
	__bss_start__ = .;
	.bss : { *(.bss) } >ram
	__bss_end__ = .;
}
